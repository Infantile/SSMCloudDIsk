<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<!-- 修改文件对象名称模型 -->
	<div class="modal" id="updateFileNameModal" tabindex="-1" role="dialog"
		aria-labelledby="myModalLabel" data-backdrop="static" data-keyboard="false" aria-hidden="true">
		<div class="modal-dialog modal-md">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal"
						aria-hidden="true">&times;</button>
					<h4>修改文件对象名</h4>
				</div>
				<div class="modal-body">
					<form class="form-horizontal">
						<div class="form-group">
							<label class="col-sm-4 control-label">当前文件对象名:</label>
							<div class="col-sm-6">
								<input type="text" readonly="true" class="form-control"
									id="currentFileName">
							</div>
						</div>
						<div class="form-group">
							<label for="inputFileName" class="col-sm-4 control-label">修改文件对象名:</label>
							<div class="col-sm-6">
								<input type="text" class="form-control" id="newFileName" maxlength="40"
									placeholder="请输入文件对象名">
							</div>							
						</div>
						<div class="form-group">
							<div class="col-sm-1"></div>
							<div class="col-sm-11" style="margin-top: 5px" id="checkFileNameAlert" name="checkFileNameAlert"></div>
						</div>
					</form>
				</div>
				<div style="background: #F0F7FC" class="modal-footer">	
					<button type="button" class="btn btn-primary" id="updateFileName">确定</button>				
					<button type="button" class="btn btn-primary"
						style="margin-left: 20px" data-dismiss="modal">取消</button>
					
				</div>
			</div>		
		</div>
	</div>
</body>
</html>