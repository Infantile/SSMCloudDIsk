<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<div id="recycleContent" style="display: none" class="row">
		<div class="col-lg-12">
			<div class="row">
				<div class="col-lg-12">
					<ol class="breadcrumb">
						<li><i class="fa fa-home"></i>主页</li>
						<li class="active"><span>回收</span></li>
					</ol>
					<h2>回收</h2>
				</div>
			</div>
			<!-- 回收站表 -->
			<div id="recycleTab" class="row">
				<div class="col-lg-12">
					<div class="main-box clearfix">
						<header class="main-box-header clearfix">
						    <div class="buttons-responsive clearfix">
						        <div class="filter-block pull-left" style="width:60%">
						            <div class="pull-left">
								        <button id="recycleBtn_restore" type="button"
									            class="btn btn-primary">
									   		<i class="fa fa-reply-all"></i>
									            	恢&nbsp;&nbsp;复</button>
							        </div>
							        <div class="pull-left" style="width: 3%;min-height: 1px"></div>
							        <div class="pull-left">
								        <button id="recycleBtn_destory" type="button"
									            class="btn btn-danger">
									        <i class="fa fa-trash-o"></i>
									            	彻底删除</button>
							        </div>
							        <div class="pull-left" style="width: 3%;min-height: 1px"></div>
							        <div class="pull-left">
								        <button id="recycleBtn_destoryAll" type="button"
									            class="btn btn-danger">
									        <i class="fa fa-trash-o"></i>
									            	清&nbsp;&nbsp;空</button>
							        </div>
						        </div>
						        <div class="filter-block pull-right">
								     <div class="form-group pull-left">
									     <input id="recycleFileSearch" type="text" class="form-control" placeholder="搜索...">
									     <i class="fa fa-search search-icon"></i>
								     </div>
								     <a id="recycleRefreshTable" class="btn btn-primary pull-right">
									     <i class="fa fa-refresh fa-lg"></i> 刷新
								    </a>
							   </div>
						    </div>
						</header>
						<div class="main-box-body clearfix" style="margin-top: -10px">
							<div class="table-responsive clearfix">
								<table class="table table-striped table-hover" style="margin-top: -10px" 
									id="tb_recyclefileInfo"></table>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>